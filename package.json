{
  "name": "soundbot",
  "version": "2.0.0",
  "description": "Soundboard bot for Discord",
  "main": "app.js",
  "author": "Tim Douglas",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurotech/soundbot.git"
  },
  "scripts": {
    "dev": "http-server build",
    "watch": "watch --interval 1 \"node build.js\" src/",
    "start": "node app.js",
    "deploy":
      "now --public --local-config now.json && now alias --local-config now.json",
    "clean": "now --local-config now.json rm soundbot --safe --yes"
  },
  "dependencies": {
    "discord.js": "11.3.2",
    "ffmpeg-binaries": "3.2.2-3",
    "log-symbols": "2.2.0",
    "node-opus": "0.3.0",
    "sea-lion": "2.0.2",
    "sentence-generator": "1.0.1",
    "spacetime": "4.1.1"
  }
}

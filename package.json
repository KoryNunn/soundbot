{
  "name": "soundbot",
  "version": "3.0.1",
  "description": "Soundboard bot for Discord",
  "main": "app.js",
  "author": "Tim Douglas",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurotech/soundbot.git"
  },
  "scripts": {
    "watch": "watch --interval 1 \"node build.js\" src/",
    "start": "node app.js",
    "predeploy": "set NODE_ENV=production&&node build.js",
    "deploy": "now --public --local-config now.json && now alias --local-config now.json",
    "clean": "now --local-config now.json rm soundbot --safe --yes"
  },
  "dependencies": {
    "authom": "0.4.36",
    "cpjax": "1.1.3",
    "dion": "2.0.0",
    "discord.js": "11.3.2",
    "ffmpeg-binaries": "3.2.2-3",
    "log-symbols": "2.2.0",
    "lowdb": "1.0.0",
    "node-opus": "0.3.0",
    "node-schedule": "1.3.0",
    "pluralize": "^7.0.0",
    "queue": "4.5.0",
    "rita": "1.3.85",
    "sea-lion": "2.0.2",
    "socket.io": "2.1.1",
    "spacetime": "4.4.0",
    "tiny-json-http": "7.0.0",
    "vaporwave": "1.0.4"
  },
  "devDependencies": {
    "browserify": "16.2.3",
    "countup.js": "1.9.3",
    "mkdirp": "0.5.1",
    "modern-normalize": "0.5.0",
    "node-sass": "4.9.3",
    "pug": "2.0.3",
    "tabler-ui": "0.0.32",
    "uglifyify": "5.0.1",
    "vue": "2.5.17"
  }
}

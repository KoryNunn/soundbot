{
  "name": "soundbot",
  "version": "2.0.0",
  "description": "Soundboard bot for Discord",
  "main": "app.js",
  "author": "Tim Douglas",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neurotech/soundbot.git"
  },
  "scripts": {
    "dev": "http-server build",
    "watch": "watch --interval 1 \"node build.js\" src/",
    "start": "node app.js",
    "predeploy": "node build.js",
    "deploy": "now --public --local-config now.json && now alias --local-config now.json",
    "clean": "now --local-config now.json rm soundbot --safe --yes"
  },
  "dependencies": {
    "discord.js": "11.3.2",
    "ffmpeg-binaries": "3.2.2-3",
    "log-symbols": "2.2.0",
    "node-opus": "0.3.0",
    "sea-lion": "2.0.2",
    "sentence-generator": "1.0.1",
    "spacetime": "4.1.1",
    "tiny-json-http": "7.0.0"
  },
  "devDependencies": {
    "countup.js": "^1.9.3",
    "mkdirp": "^0.5.1",
    "modern-normalize": "^0.4.0",
    "node-sass": "^4.9.0",
    "pug": "^2.0.3",
    "tabler-ui": "0.0.32",
    "uglify-js": "^3.3.25",
    "vue": "^2.5.16"
  }
}
